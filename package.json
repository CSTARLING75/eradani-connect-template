{
  "name": "eradani-connect-template",
  "version": "2.1.0",
  "description": "Eradani Connect Starter Application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "test": "mocha ./tests",
    "install-deps": "yum install git make-gnu unzip -y && npm install -g pm2 && ln -s /QOpenSys/pkgs/lib/nodejs10/bin/pm2 /QOpenSys/pkgs/bin/pm2",
    "build": "export LIB=ECNCTDEMO && cd native && gmake init && gmake clean && gmake && cd ..",
    "update-native": "git pull && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eradani-inc/eradani-connect-template.git"
  },
  "author": "Eradani",
  "homepage": "https://github.com/eradani-inc/eradani-connect-template#readme",
  "dependencies": {
    "@eradani-inc/eradani-connect": "^2.1.3",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.0",
    "express-validator": "^5.3.0",
    "itoolkit": "^0.1.6",
    "jsonwebtoken": "^8.5.0",
    "morgan": "^1.9.0",
    "odbc": "^2.2.2",
    "sequelize": "^5.21.3",
    "winston": "^3.2.0",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "mocha": "^6.1.0",
    "prettier": "^1.17.1"
  }
}
